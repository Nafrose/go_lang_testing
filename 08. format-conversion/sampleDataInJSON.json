{
   "log4go":{
      "root":{
         "level":{
            "-value":"ALL"
         },
         "appender-ref":[
            {
               "-ref":"ColorConsoleAppender"
            },
            {
               "-ref":"InfoFileAppender"
            },
            {
               "-ref":"DebugFileAppender"
            },
            {
               "-ref":"WarningFileAppender"
            },
            {
               "-ref":"ErrorFileAppender"
            },
            {
               "-ref":"FatalFileAppender"
            },
            {
               "-ref":"DiagnosticFileAppender"
            },
            {
               "-ref":"Sqlite3Db"
            }
         ],
         "broad-casting-appender":{
            "-ref":"HttpServingUsingJson"
         },
         "system":{
            "-mode":"develop",
            "-is-enable-json-error":"false"
         },
         "variable-creator":{
            "variable":[
               {
                  "-name":"logs-root-path",
                  "-value":"."
               },
               {
                  "-name":"logs-path",
                  "-value":"%logs-root-path%/logs"
               },
               {
                  "-name":"common-logging-pattern",
                  "-value":"%common-logging-pattern%"
               }
            ]
         },
         "clear-config-appender":{
            "-name":"clear-on-develop"
         }
      },
      "clear-config":{
         "-name":"clear-on-develop",
         "-enables-on-system-mode":"develop",
         "run":{
            "-index":"1"
         },
         "total-run":{

         },
         "on-next-run":{

         },
         "on-every-third-run":{

         }
      },
      "appender":[
         {
            "-name":"ColorConsoleAppender",
            "-type":"ColoredConsoleAppender",
            "mapping":[
               {
                  "level":{
                     "-value":"ERROR"
                  },
                  "foreColor":{
                     "-value":"White"
                  },
                  "backColor":{
                     "-value":"Red"
                  }
               },
               {
                  "level":{
                     "-value":"DEBUG"
                  },
                  "backColor":{
                     "-value":"Yellow"
                  }
               },
               {
                  "level":{
                     "-value":"INFO"
                  },
                  "foreColor":{
                     "-value":"White"
                  }
               },
               {
                  "level":{
                     "-value":"WARN"
                  },
                  "foreColor":{
                     "-value":"White"
                  },
                  "backColor":{
                     "-value":"Purple"
                  }
               }
            ],
            "layout":{
               "-type":"PatternLayout",
               "conversionPattern":{
                  "-value":"%date %level %logger - %message%newline"
               }
            }
         },
         {
            "-name":"DateTimeInfoFileAppender",
            "-type":"DateTimeSizeRollingFileAppender",
            "file":{
               "-value":"%logs-path%/%i3% . dated-%week%-%month%-%year%.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Weekly"
            },
            "maxSizeRollBackups":{
               "-value":"5"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":{
               "-type":"LevelMatchFilter",
               "acceptOnMatch":{
                  "-value":"true"
               },
               "levelToMatch":{
                  "-value":"INFO"
               }
            },
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"InfoFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/info.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"10"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":[
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"INFO"
                  }
               },
               {
                  "-type":"log4go.Filter.DenyAllFilter"
               }
            ],
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"DebugFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/debug.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"10"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":[
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"DEBUG"
                  }
               },
               {
                  "-type":"log4go.Filter.DenyAllFilter"
               }
            ],
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"WarningFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/warning.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"10"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":[
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"WARN"
                  }
               },
               {
                  "-type":"log4go.Filter.DenyAllFilter"
               }
            ],
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"ErrorFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/all.errors.warning.fatal.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"10"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":[
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"ERROR"
                  }
               },
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"FATAL"
                  }
               },
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"WARN"
                  }
               },
               {
                  "-type":"log4go.Filter.DenyAllFilter"
               }
            ],
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"FatalFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/fatal.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"10"
            },
            "maximumFileSize":{
               "-value":"40MB"
            },
            "filter":[
               {
                  "-type":"LevelMatchFilter",
                  "acceptOnMatch":{
                     "-value":"true"
                  },
                  "levelToMatch":{
                     "-value":"FATAL"
                  }
               },
               {
                  "-type":"log4go.Filter.DenyAllFilter"
               }
            ],
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"DiagnosticFileAppender",
            "-type":"RollingFileAppender",
            "file":{
               "-value":"%logs-path%/diagnostic.all.logs"
            },
            "appendToFile":{
               "-value":"true"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"2"
            },
            "maximumFileSize":{
               "-value":"10MB"
            },
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         },
         {
            "-name":"Sqlite3Db",
            "-type":"Sqlite3DbLogger",
            "file":{
               "-value":"%logs-path%/sqlitedb.logs.db"
            },
            "archive-file":{
               "-name":"%logs-path%/sqlite3-arc/%i3% . sqlitedb.logs.db"
            },
            "appendToFile":{
               "-value":"false"
            },
            "rollingStyle":{
               "-value":"Size"
            },
            "maxSizeRollBackups":{
               "-value":"2"
            },
            "maximumFileSize":{
               "-value":"10MB"
            },
            "staticLogFileName":{
               "-value":"false"
            },
            "layout":{
               "-type":"PatternLayout",
               "param":{
                  "-name":"ConversionPattern",
                  "-value":"%common-logging-pattern%"
               }
            }
         }
      ],
      "auth":{
         "-name":"basic",
         "role":[
            {
               "-name":"admin",
               "-priority":"0",
               "user":[
                  {
                     "-name":"admin1",
                     "-is-changed":"true",
                     "-hash-pass-after-single-run":"true",
                     "-pass":"@*onetime-pass53"
                  },
                  {
                     "-name":"admin2",
                     "-is-changed":"true",
                     "-hash-pass-after-single-run":"true",
                     "-pass":"@*onetime-pass75"
                  }
               ]
            },
            {
               "-name":"developer",
               "-priority":"0",
               "user":[
                  {
                     "-name":"developer1",
                     "-is-changed":"true",
                     "-hash-pass-after-single-run":"true",
                     "-pass":"@*onetime-pass53"
                  },
                  {
                     "-name":"developer2",
                     "-is-changed":"true",
                     "-hash-pass-after-single-run":"true",
                     "-pass":"@*onetime-pass75"
                  }
               ]
            }
         ]
      },
      "email-config":{
         "-name":"gmail1"
      },
      "trigger-service":{
         "-name":"email-phone",
         "-email-config":"gmail1",
         "-trigger-on":"weekly"
      },
      "broadcasting":{
         "-name":"HttpServingUsingJson",
         "-type":"HttpServingUsingJson",
         "source":{
            "-appender-name":"Sqlite3Db",
            "-appender-type":"Sqlite3DbLogger"
         },
         "serve":{
            "-on":"localhost",
            "-port":"4325",
            "-additional-suffix":"/logging/remote",
            "-is-auth":"true",
            "-is-https":"false",
            "auth-use":{
               "-name":"basic"
            }
         },
         "trigger":{
            "-name":"email-phone"
         }
      },
      "config-mutator":{
         "serve":{
            "-on":"localhost",
            "-port":"4326",
            "-additional-suffix":"/logging/configure",
            "-is-auth":"true",
            "-is-https":"false",
            "auth":{
               "role":{
                  "-name":"admin",
                  "-priority":"0",
                  "user":[
                     {
                        "-name":"admin1",
                        "-is-changed":"true",
                        "-hash-pass-after-single-run":"true",
                        "-pass":"@*onetime-pass53"
                     },
                     {
                        "-name":"admin2",
                        "-is-changed":"true",
                        "-hash-pass-after-single-run":"true",
                        "-pass":"@*onetime-pass75"
                     }
                  ]
               }
            }
         }
      }
   }
}